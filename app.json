{
  "expo": {
    "name": "الدجاج الملكي بروست",
    "slug": "almalaky-broast-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "malakyapp",
    "userInterfaceStyle": "light",
    "icon": "./assets/images/icon.jpg",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icon.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#D32F2F"
    },
    "ios": {
      "supportsTablet": true,
      "supportsRTL": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.jpg",
        "backgroundColor": "#D32F2F"
      },
      "supportsRtl": true,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.x3asoo1.malakyapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b83c8c0f-913e-41a4-b35a-ce1f29aa4eef"
      }
    }
  }
}
