{
  "expo": {
    "name": "الدجاج الملكي بروست",
    "slug": "almalaky-broast-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "malakyapp",
    "userInterfaceStyle": "light",
    "icon": "./assets/images/icon.png",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.x3asoo1.malakyapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-localization",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b83c8c0f-913e-41a4-b35a-ce1f29aa4eef"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/b83c8c0f-913e-41a4-b35a-ce1f29aa4eef"
    }
  }
}
